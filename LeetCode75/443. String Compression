#from collections import defaultdict
class Solution(object):
    def compress(self, chars):
        """
        :type chars: List[str]
        :rtype: int
        """
#        dic=defaultdict(int)
#        print(dic)
        arr = []
        cnt, now = 1, chars[0]
        for i in range(1, len(chars)):
            if chars[i] == now:
                cnt += 1
            else:
                arr.append(str(now))
                arr.append(str(cnt))
                now = chars[i]
                cnt  = 1
        arr.append(str(now))
        arr.append(str(cnt))
        print(arr)
        chars = arr
        return len(arr)

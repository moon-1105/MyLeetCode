class Solution:
    def cal(self, num):
        if num < 5 :
            return "I"*num
        return ""

    def intToRoman(self, num: int) -> str:
        nums = []
        while num > 10 :
            nums.append(num%10)
            num = int(num/10)
        nums.append(num)
        
        ans = ""
        for idx, num in enumerate(nums) :
            ans = self.cal(num * int(math.pow(10,idx))) + ans

        return ans

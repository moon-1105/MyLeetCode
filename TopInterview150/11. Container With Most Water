class Solution:
    def maxArea(self, height: List[int]) -> int:
        left, right = 0, len(height)-1
        Max = 0
        while left < right :
            now = min(height[left], height[right]) * (right-left)
            if Max < now :
                print(left, right)
                Max = now
                left += 1
            else :
                right -= 1

        return Max

        
